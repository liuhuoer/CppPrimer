#include "ex13_31.h"
#include <algorithm>

//rewrite_:(const HasPtr &) change to (HasPtr rhp)
HasPtr & HasPtr::operator=(HasPtr rhp)
{
	std::cout<<"test_="<<std::endl;
	//using std::swap;
	//swap(*this,rhp);	//rewrite_:use swap();
	this->swap(rhp);

	return *this;
}

void HasPtr::output()
{
	std::cout<<*ps<<std::endl;
}

int main()
{
	HasPtr a("c"),b("b"),c("a");
	vector<HasPtr> vh{a,b,c};
	//for(string s;std::cin>>s;)
	//	vh.emplace_back(s);

	std::sort(vh.begin(),vh.end());

	for(auto a:vh)
		a.output();

	return 0;
}
